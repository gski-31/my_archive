{
  "extends": [
    "plugin:@waybetter/waybetter/recommended"
  ],
  "plugins": [
    "@waybetter/waybetter",
    "jest"
  ],
  "settings": {},
  "env": {
    "jest/globals": true
  },
  "globals": {
    "shallow": true
  },
  "overrides": [
    {
      "files": [
        "*.js"
      ],
      "rules": {
        "import/no-unresolved": "off",
        "no-console": "off"
      }
    }
  ]
}