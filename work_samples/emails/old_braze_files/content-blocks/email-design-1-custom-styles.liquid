        .wbContent {
            padding: 1rem;
        }
    }

    .wbHeaderLogoImage {
        max-height: 40px;
    }

    .wbHeaderLogoImageContent {
        text-align: center;
        padding: 9px 0 18px;
    }

    h2 {
        font-size: 22px;
        text-align: center;
